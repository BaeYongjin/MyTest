<?xml version="1.0" encoding="utf-8" ?> 
<xs:schema id="GradeDs" 
                  targetNamespace="http://tempuri.org/GradeDs.xsd"
                  elementFormDefault="qualified"
                  attributeFormDefault="qualified"
                  xmlns="http://tempuri.org/GradeDs.xsd"
                  xmlns:mstns="http://tempuri.org/GradeDs.xsd"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
		<xs:element name="GradeDs" msdata:IsDataSet="true">
		<xs:complexType>
				<xs:choice maxOccurs="unbounded">
				<xs:element name="Medias">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MediaCode" type="xs:string" minOccurs="0" />
							<xs:element name="MediaName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Gradecode">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Code" type="xs:string" minOccurs="0" />
								<xs:element name="CodeName" type="xs:string" minOccurs="0" />											
							</xs:sequence>
						</xs:complexType>
				</xs:element>
				<xs:element name="Grade">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Code" type="xs:string" minOccurs="0" />
								<xs:element name="CodeName" type="xs:string" minOccurs="0" />											
								<xs:element name="Grade" type="xs:string" minOccurs="0" />											
								<xs:element name="RegID" type="xs:string" minOccurs="0" />											
								<xs:element name="RegDt" type="xs:string" minOccurs="0" />											
								<xs:element name="ModDt" type="xs:string" minOccurs="0" />											
							</xs:sequence>
						</xs:complexType>
				</xs:element>
				<xs:element name="ContractItem">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ItemNo" type="xs:int" minOccurs="0" />
							<xs:element name="ItemName" type="xs:string" minOccurs="0" />
							<xs:element name="ContractName" type="xs:string" minOccurs="0" />
							<xs:element name="AdvertiserCode" type="xs:string" minOccurs="0" />
							<xs:element name="AdvertiserName" type="xs:string" minOccurs="0" />
							<xs:element name="ExcuteStartDay" type="xs:string" minOccurs="0" />
							<xs:element name="ExcuteEndDay" type="xs:string" minOccurs="0" />
							<xs:element name="RealEndDay" type="xs:string" minOccurs="0" />
							<xs:element name="AdState" type="xs:string" minOccurs="0" />
							<xs:element name="AdStateName" type="xs:string" minOccurs="0" />
							<xs:element name="AdClass" type="xs:string" minOccurs="0" />
							<xs:element name="FilePath" type="xs:string" minOccurs="0" />
							<xs:element name="FileName" type="xs:string" minOccurs="0" />
							<xs:element name="FileState" type="xs:string" minOccurs="0" />
							<xs:element name="FileStateName" type="xs:string" minOccurs="0" />
							<xs:element name="HomeCount" type="xs:int" minOccurs="0" />
							<xs:element name="MenuCount" type="xs:int" minOccurs="0" />
							<xs:element name="ChannelCount" type="xs:int" minOccurs="0" />
							<xs:element name="NowDay" type="xs:string" minOccurs="0" />
							<xs:element name="AdType" type="xs:string" minOccurs="0" />
							<xs:element name="AdTypeName" type="xs:string" minOccurs="0" />
							<xs:element name="LinkChannel" type="xs:string" minOccurs="0" />
							<xs:element name="LinkChannelNm" type="xs:string" minOccurs="0" />
							<xs:element name="Mgrade" type="xs:string" minOccurs="0" />
							<xs:element name="HomeYn" type="xs:string" minOccurs="0" />
							<xs:element name="ChannelYn" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
