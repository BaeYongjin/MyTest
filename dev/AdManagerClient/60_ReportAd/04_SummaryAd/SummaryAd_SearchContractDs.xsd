<?xml version="1.0" encoding="utf-8" ?> 
<xs:schema id="SummaryAd_SearchContractDs" 
                  targetNamespace="http://tempuri.org/SummaryAd_SearchContractDs.xsd"
                  elementFormDefault="qualified"
                  attributeFormDefault="qualified"
                  xmlns="http://tempuri.org/SummaryAd_SearchContractDs.xsd"
                  xmlns:mstns="http://tempuri.org/SummaryAd_SearchContractDs.xsd"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="SummaryAd_SearchContractDs" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Medias">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MediaCode" type="xs:string" minOccurs="0" />
							<xs:element name="MediaName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="MediaRaps">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="RapCode" type="xs:string" minOccurs="0" />
							<xs:element name="RapName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Agencys">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AgencyCode" type="xs:string" minOccurs="0" />
							<xs:element name="AgencyName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Contract">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AgencyName" type="xs:string" minOccurs="0" />
							<xs:element name="AdvertiserName" type="xs:string" minOccurs="0" />
							<xs:element name="ContractSeq" type="xs:int" minOccurs="0" />
							<xs:element name="ContractName" type="xs:string" minOccurs="0" />
							<xs:element name="ContStartDay" type="xs:string" minOccurs="0" />
							<xs:element name="ContEndDay" type="xs:string" minOccurs="0" />
							<xs:element name="ContractAmt" type="xs:string" minOccurs="0" />
							<xs:element name="State" type="xs:string" minOccurs="0" />
							<xs:element name="StateName" type="xs:string" minOccurs="0" />
							<xs:element name="AdCount" type="xs:int" minOccurs="0" />
						</xs:sequence>						
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
