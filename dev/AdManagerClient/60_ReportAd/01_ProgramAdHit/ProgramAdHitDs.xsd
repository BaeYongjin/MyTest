<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="ProgramAdHitDs" targetNamespace="http://tempuri.org/ProgramAdHitDs.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/ProgramAdHitDs.xsd" xmlns:mstns="http://tempuri.org/ProgramAdHitDs.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="ProgramAdHitDs" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Media">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MediaCode" type="xs:string" minOccurs="0" />
							<xs:element name="MediaName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Report">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CategoryCode" type="xs:string" minOccurs="0" />
							<xs:element name="CategoryName" type="xs:string" minOccurs="0" />
							<xs:element name="GenreCode" type="xs:string" minOccurs="0" />
							<xs:element name="GenreName" type="xs:string" minOccurs="0" />
							<xs:element name="ChannelNo" type="xs:int" minOccurs="0" />
							<xs:element name="ProgramNm" type="xs:string" minOccurs="0" />
							<xs:element name="AdHitCnt" type="xs:int" minOccurs="0" />
							<xs:element name="PgHitCnt" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Items">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ItemNo" type="xs:string" minOccurs="0" />
							<xs:element name="ItemName" type="xs:string" minOccurs="0" />
							<xs:element name="ExcuteStartDay" type="xs:string" minOccurs="0" />
							<xs:element name="RealEndDay" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Campaign">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CampaignCode" type="xs:string" minOccurs="0" />
							<xs:element name="CampaignName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>