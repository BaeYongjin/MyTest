<?xml version="1.0" encoding="utf-8" ?> 
<xs:schema id="StatisticsTimeDs" 
                  targetNamespace="http://tempuri.org/StatisticsTimeDs.xsd"
                  elementFormDefault="qualified"
                  attributeFormDefault="qualified"
                  xmlns="http://tempuri.org/StatisticsTimeDs.xsd"
                  xmlns:mstns="http://tempuri.org/StatisticsTimeDs.xsd"
                  xmlns:xs="http://www.w3.org/2001/XMLSchema"
                  xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="StatisticsTimeDs" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Media">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MediaCode" type="xs:string" minOccurs="0" />
							<xs:element name="MediaName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Campaign">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CampaignCode" type="xs:string" minOccurs="0" />
							<xs:element name="CampaignName" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Report">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TimeOrder" type="xs:string" minOccurs="0" />
							<xs:element name="TimeName" type="xs:string" minOccurs="0" />
							<xs:element name="AdCnt" type="xs:int" minOccurs="0" />
							<xs:element name="AdRate" type="xs:float" minOccurs="0" />
							<xs:element name="RateBar" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Items">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ItemNo" type="xs:string" minOccurs="0" />
							<xs:element name="ItemName" type="xs:string" minOccurs="0" />
							<xs:element name="ExcuteStartDay" type="xs:string" minOccurs="0" />
							<xs:element name="RealEndDay" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>
