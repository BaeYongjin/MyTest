    <!-- ********************* -->
    <!-- DB 처리 Return Code 정의 -->
    <add key="DBSuccess" value="0000" />
    <add key="DBListFail" value="3000" />
    <add key="DBAddFail" value="3101" />
    <add key="DBEditFail" value="3201" />
    <add key="DBDelFail" value="3301" />
